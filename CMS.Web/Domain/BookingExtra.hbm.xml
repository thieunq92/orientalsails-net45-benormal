<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="CMS.Web.Domain"
                   assembly="CMS.Web">
  <class name="BookingExtra" table="os_BookingExtra" batch-size="10">
    <cache usage="read-write"/>
    <id name="Id">
      <generator class="native" />
    </id>

    <many-to-one name="Booking" column="BookingId" class="Booking"/>
    <many-to-one name="ExtraOption" column="ExtraOptionId" class="ExtraOption"/>
  </class>
</hibernate-mapping>
